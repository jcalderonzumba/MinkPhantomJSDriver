{
  "name": "behat/mink-phantomjs-driver",
  "description": "PhantomJS driver for Mink framework",
  "keywords": [
    "phantomjs",
    "headless",
    "javascript",
    "ajax",
    "testing",
    "browser"
  ],
  "homepage": "http://mink.behat.org/",
  "type": "mink-driver",
  "license": "MIT",
  "authors": [
    {
      "name": "Juan Francisco CalderÃ³n Zumba",
      "email": "juanfcz@gmail.com",
      "homepage": "http://github.com/jcalderonzumba"
    }
  ],
  "require": {
    "php": ">=5.3.1",
    "behat/mink": "~1.6",
    "behat/mink-extension": "~2.0@dev",
    "twig/twig": "~1.0",
    "guzzlehttp/guzzle": "~5.0"
  },
  "require-dev": {
    "symfony/process": "~2.0",
    "phpunit/phpunit": "~4.6",
    "silex/silex": "~1.2"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Behat\\PhantomJSExtension\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Behat\\PhantomJSExtension\\Tests\\": "tests/unit",
      "Behat\\Mink\\Tests\\Driver\\": "tests/integration"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
